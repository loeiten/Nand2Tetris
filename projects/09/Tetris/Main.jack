/** Initializes a new Tetris game and starts running it. */
class Main {
  function void main() {
    var TetrisGame game;
    var SplashScreen splash;
    var int seed;

    let splash = SplashScreen.new();
    let seed = splash.printSplashScreen();
    let game = TetrisGame.new(seed);
    do game.run();

    do splash.dispose();
    do game.dispose();
    return;
  }
}
